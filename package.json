    "test": "tests"
  },
  "scripts": {
    "build-ts": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/server-app.js",
    "test": "cross-env NODE_ENV=unitTest nyc --reporter=lcov --reporter=text mocha --require ts-node/register --timeout 99999 --recursive 'tests/**/*.spec.ts' --exit",
    "coverage": "nyc report --reporter=lcov",
    "watch-node": "nodemon --ignore public/ --inspect=127.0.0.1:9222 dist/server-app.js -r newrelic newrelic.js",
    "watch-ts": "tsc --watch"
  },
